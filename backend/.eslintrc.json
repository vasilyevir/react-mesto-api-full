{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12
    },
    "plugins": [
    ],
    "extends": "airbnb-base",
    "rules": {
      "no-underscore-dangle": ["error", { "allow": ["_id"] }],
      "no-unused-vars": ["error", { "vars": "all", "args": "none", "ignoreRestSiblings": false }],
      "no-console": ["error", {"allow": ["log"]}]
    }
}
